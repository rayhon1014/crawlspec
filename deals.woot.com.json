{
  "name":"woot.com forum deals for geek",
  "model": "com.fp.ecommerce.model.Deal",
  "urls":[
    "index:http://deals.woot.com/ !! urlPath=div[class~=.*postInfo.*] h3 a/@href"

  ],
  "list_path":"div[class=primaryContent]",
  "fields":{
    "siteName": "##deals.woot.com",
    "title": "span[class*=fn]",
    "category": "li[class*=first] a[class*=category]",
    "dealPrice": "span[class*=amount]",
    "finalUrl": "div[class~=postInfo.*] h3 a/@href",
    "imageUrl": "div[class~=postInfo.*] h3 a > img/@src",
    "desc": "div[class*=description] p",
    "numOfLike": "span[class*=count]"
  }
}
